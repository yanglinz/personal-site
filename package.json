{
  "name": "personal-site",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf _site",
    "build:11ty": "eleventy",
    "build:parcel": "parcel build --dist-dir=_site src/main.js",
    "build:tailwind": "tailwindcss -i ./src/main.css -o ./_site/main.css --jit --minify",
    "build": "run-s clean build:*",
    "start:11ty": "eleventy --serve",
    "start:parcel": "parcel watch --dist-dir=_site src/main.js",
    "start:tailwind": "tailwindcss -i ./src/main.css -o ./_site/main.css --jit --watch",
    "start": "run-p start:*",
    "format": "prettier --write './*.js' './{lib,src,www}/**/*.{js,cjs,mjs,css,html,md,json}'"
  },
  "dependencies": {
    "@11ty/eleventy": "1.0.1",
    "@11ty/eleventy-img": "2.0.1",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.0.0",
    "date-fns": "2.27.0",
    "nunjucks": "3.2.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "autoprefixer": "10.2.6",
    "jest": "26.4.2",
    "npm-run-all": "4.1.5",
    "parcel": "^2.4.1",
    "postcss": "8.3.5",
    "prettier": "2.1.1",
    "prettier-plugin-tailwind": "2.2.12",
    "tailwindcss": "2.2.4"
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.4"
  }
}
