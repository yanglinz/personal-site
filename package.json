{
  "name": "personal-site",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:tailwind": "tailwindcss -i ./src/main.css -o ./_site/main.css --jit --minify",
    "build:11ty": "eleventy",
    "build": "run-s build:*",
    "start:tailwind": "tailwindcss -i ./src/main.css -o ./_site/main.css --jit --watch",
    "start:11ty": "eleventy --serve",
    "start": "run-p start:*",
    "format": "prettier --write './{content,contrib,pages,src}/**/*.{js,ts,tsx,css,html,md,json}'"
  },
  "dependencies": {
    "@11ty/eleventy": "1.0.0-beta.8",
    "@11ty/eleventy-img": "1.0.0",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.1.3",
    "date-fns": "2.27.0",
    "nunjucks": "3.2.3"
  },
  "devDependencies": {
    "autoprefixer": "10.2.6",
    "jest": "26.4.2",
    "npm-run-all": "4.1.5",
    "parcel": "^2.4.1",
    "postcss": "8.3.5",
    "prettier": "2.1.1",
    "prettier-plugin-tailwind": "2.2.12",
    "tailwindcss": "2.2.4"
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.4"
  }
}
