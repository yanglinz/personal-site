---
import type { CollectionEntry } from "astro:content";
import { imageMarkup, getFeaturedImagePath } from "./ImageHelper";
import { Image as ImageAlt } from 'astro-eleventy-img';

interface Props {
  slug: string;
  data: CollectionEntry<"posts">;
}

const { slug, data } = Astro.props;
const src = await getFeaturedImagePath(slug, data);
// const imageHtml = await imageMarkup(src, "TODO!");
---

<ImageAlt src={src} alt="123" />